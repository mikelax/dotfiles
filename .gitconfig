[user]
        name = Michael Holtzman
        email = mholtzman@gmail.com
[github]
        user = mikelax
[core]
        excludesfile = ~/.gitignore_global
        editor = vim
        pager = less
[push]
        default = simple
[color]
        ui = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
	deleted = red
[branch]
        autosetuprebase = always
